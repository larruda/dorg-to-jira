{
  "name": "larruda/dorg-to-jira",
  "description": "A simple PHP script to import issues from Drupal.org into a Jira project.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lucas Arruda",
      "email": "lunascar+composer@gmail.com"
    }
  ],
  "require": {
    "symfony/console": "~3.1",
    "EclipseGc/drupal-org-api": "dev-master",
    "chobie/jira-api-restclient": "~1.0",
    "symfony/yaml": "~3.1"
  },
  "autoload": {
    "psr-4": {
      "Larruda\\DorgToJira\\": "src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/larruda/drupal-org-api"
    }
  ],
  "bin": [
    "cli.php",
    "bin/dorg-to-jira"
  ]
}
